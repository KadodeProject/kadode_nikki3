{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^3.6",
        "goodby/csv": "^1.3",
        "google/cloud-storage": "^1.30",
        "grpc/grpc": "^1.52",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "spatie/laravel-backup": "^8.0",
        "symfony/mailgun-mailer": "^6.0",
        "torann/geoip": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-ide-helper": "^2.12",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.9",
        "laravel-lang/lang": "^12.19",
        "laravel/dusk": "^7.7",
        "laravel/sail": "^1.21",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "GrpcClient\\": "GrpcClient/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "git config --local core.hooksPath .githooks"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "cs-fixer": [
            "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php --diff --dry-run"
        ],
        "cs-fixer-verbose": [
            "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php -vv --dry-run"
        ],
        "cs-fixer-fix": [
            "./vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php -v"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse --memory-limit=2g"
        ],
        "phpstan-g": [
            "./vendor/bin/phpstan analyse --memory-limit=2g --generate-baseline "
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
