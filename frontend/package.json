{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"tests": "pnpm test:u & pnpm test:c & pnpm test:i",
		"dev": "vite dev --port 2000 --host",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:u": "vitest run",
		"test:c": "playwright test -c playwright-ct.config.ts",
		"test:i": "playwright test -c playwright.config.ts"
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.34.3",
		"@playwright/test": "^1.34.3",
		"@poppanator/sveltekit-svg": "^3.0.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.2.4",
		"@sveltejs/kit": "^1.19.0",
		"@types/set-cookie-parser": "^2.4.2",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^22.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.2",
		"typescript": "^5.0.4",
		"vite": "^4.3.8",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"axios": "^1.4.0",
		"dayjs": "^1.11.7",
		"set-cookie-parser": "^2.6.0"
	}
}
