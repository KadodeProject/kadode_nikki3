{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"tests": "pnpm test:u & pnpm test:c & pnpm test:i",
		"dev": "vite dev --port 2000 --host",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:u": "vitest run",
		"test:c": "playwright test -c playwright-ct.config.ts",
		"test:i": "playwright test -c playwright.config.ts"
	},
	"devDependencies": {
		"@playwright/experimental-ct-svelte": "^1.36.2",
		"@playwright/test": "^1.36.2",
		"@poppanator/sveltekit-svg": "^3.0.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.4",
		"@testing-library/svelte": "^3.2.2",
		"@types/node-fetch": "^2.6.4",
		"@types/set-cookie-parser": "^2.4.3",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jsdom": "^22.1.0",
		"openapi2aspida": "^0.21.0",
		"postcss": "^8.4.27",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.2",
		"svelte-check": "^3.4.6",
		"tailwindcss": "^3.3.3",
		"tinyspy": "^2.1.1",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.8",
		"vitest": "^0.34.1"
	},
	"type": "module",
	"dependencies": {
		"@aspida/node-fetch": "^1.13.3",
		"axios": "^1.4.0",
		"dayjs": "^1.11.9",
		"set-cookie-parser": "^2.6.0"
	}
}
